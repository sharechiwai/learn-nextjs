{
  "extends": "next/core-web-vitals",
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": { "order": "asc" },
        "newlines-between": "always"
      }
    ],
    "space-before-function-paren": 0,
    "semi": ["error", "always"],
    "comma-dangle": [2, "always-multiline"]
  }
}
